{
  "name": "tfc-framework",
  "version": "2024.7.1",
  "dependencies": [
    {
      "name": "async-mqtt",
      "version>=": "4.0.0"
    },
    "azmq",
    "bext-sml",
    "bext-ut",
    "boost-asio",
    "boost-program-options",
    "boost-coroutine2",
    "boost-stacktrace",
    "boost-uuid",
    "date",
    "fmt",
    {
      "name": "glaze",
      "version>=": "2.6.4"
    },
    "gtest",
    "magic-enum",
    {
      "name": "mp-units",
      "version>=": "2.2.1"
    },
    "openssl",
    "pcg",
    "protobuf",
    {
      "name": "sdbusplus",
      "version>=": "2023-09-14",
      "features": [
        "asio-only"
      ]
    },
    "soem",
    "spdlog",
    "stduuid",
    "sqlite3",
    "sqlite-modern-cpp",
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ],
  "features": {
    "build-exes": {
      "description": "Build binaries and install them to sysroot"
    }
  },
  "overrides": [
    { "name": "boost-stacktrace", "version": "1.85.0", "port-version": 2 },
    { "name": "mp-units", "version": "2.2.1" },
    { "name": "glaze", "version": "2.9.2" },
    { "name": "fmt", "version": "10.2.1", "port-version": 2 },
    { "name": "async-mqtt", "version": "4.0.0" },
    { "name": "openssl", "version": "3.3.1" }
  ]
}
